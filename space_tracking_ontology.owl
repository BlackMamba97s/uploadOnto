#    Datatypes
#################################################################

###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#greekLetter
:greekLetter rdf:type rdfs:Datatype ;
             rdfs:comment "Letter of the greek aplhabet, typically used to identify a constant or a variable in Physics"@en ;
             rdfs:label "Greek Letter"@en ;
             owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                   owl:onDatatype xsd:string ;
                                   owl:withRestrictions ( [ xsd:length 1
                                                          ]
                                                        )
                                 ] .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Has_Orbit_Center
:Has_Orbit_Center rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :Orbits_Around ;
                  rdfs:domain :Astronomical_Object ,
                              :Satellite ;
                  rdfs:range :Astronomical_Object ;
                  rdfs:label "Ha centro orbitale"@it ,
                             "Has Orbit Center"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Orbited_By
:Orbited_By rdf:type owl:ObjectProperty ;
            owl:inverseOf :Orbits_Around ;
            rdfs:domain :Astronomical_Object ;
            rdfs:range :Astronomical_Object ,
                       :Satellite ;
            rdfs:label "Orbitato da"@it ,
                       "Orbited By"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Orbits_Around
:Orbits_Around rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain :Astronomical_Object ;
               rdfs:range :Astronomical_Object ,
                          :Satellite ;
               rdfs:label "Orbita Intorno"@it ,
                          "Orbits Around"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#hasMissionDestination
:hasMissionDestination rdf:type owl:ObjectProperty ;
                       owl:inverseOf :missionDestinationIncludedIn ;
                       rdfs:domain :Space_Mission ;
                       rdfs:range :Mission_Destination .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasParticipant ;
                rdfs:domain :Mission_Destination ;
                rdfs:range :Mission_Object .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#isBiggerThan
:isBiggerThan rdf:type owl:ObjectProperty ;
              owl:inverseOf :isSmallerThan ;
              rdf:type owl:TransitiveProperty ;
              rdfs:domain :Astronomical_Object ;
              rdfs:range :Astronomical_Object .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#isParticipantIn
:isParticipantIn rdf:type owl:ObjectProperty ;
                 rdfs:domain :Mission_Object ;
                 rdfs:range :Mission_Destination .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#isSmallerThan
:isSmallerThan rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#missionDestinationIncludedIn
:missionDestinationIncludedIn rdf:type owl:ObjectProperty ,
                                       owl:InverseFunctionalProperty .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#missionObjectIncludedIn
:missionObjectIncludedIn rdf:type owl:ObjectProperty ;
                         owl:inverseOf :missionObjectParticipating ;
                         rdf:type owl:InverseFunctionalProperty .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#missionObjectParticipating
:missionObjectParticipating rdf:type owl:ObjectProperty ;
                            rdfs:domain :Space_Mission ;
                            rdfs:range :Mission_Object ;
                            rdfs:comment "objectParticipating" .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#roleIncludedIn
:roleIncludedIn rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Mission_Role ;
                rdfs:range :Mission_Object ;
                rdfs:comment "abbiamo messo questa object property funzionale perchè ogni oggetto della missione ha un solo ruolo all'interno della missione, per esempio Neil Armstrong è il comandante, ma non viceversa."^^xsd:string .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#roleOfMission
:roleOfMission rdf:type owl:ObjectProperty ;
               rdfs:domain :Space_Mission ;
               rdfs:range :Mission_Role ;
               rdfs:comment "roleOfParticipation" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Argument_Of_Periapsis
:Argument_Of_Periapsis rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :Orbital_Parameter ;
                       rdfs:range xsd:double ;
                       :physicsSymbol "ω"^^:greekLetter ;
                       rdfs:comment "defines the orientation of the ellipse in the orbital plane, as an angle measured from the ascending node to the periapsis"@en ;
                       rdfs:label "Argument of Periapsis"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Eccentricity
:Eccentricity rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :Orbital_Parameter ;
              rdfs:range xsd:double ;
              :physicsSymbol "e"^^xsd:string ;
              rdfs:label "Eccentricity"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Inclination
:Inclination rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :Orbital_Parameter ;
             rdfs:range xsd:double ;
             :physicsSymbol "i"^^xsd:string ;
             rdfs:comment "Vertical tilt of the ellipse with respect to the reference plane, measured at the ascending node according to a reference plane"@en ;
             rdfs:label "Inclination"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Longitude_Of_The_Ascending_Node
:Longitude_Of_The_Ascending_Node rdf:type owl:DatatypeProperty ;
                                 rdfs:subPropertyOf :Orbital_Parameter ;
                                 rdfs:range xsd:double ;
                                 :physicsSymbol "Ω"^^:greekLetter ;
                                 rdfs:comment "horizontally orients the ascending node of the ellipse"@en ;
                                 rdfs:label "Longitude of the ascending node"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Mass
:Mass rdf:type owl:DatatypeProperty ;
      rdfs:range [ rdf:type rdfs:Datatype ;
                   owl:onDatatype xsd:double ;
                   owl:withRestrictions ( [ xsd:minExclusive "0.0"^^xsd:double
                                          ]
                                        )
                 ] ;
      rdfs:comment "The mass of a thing expressed in Kg"@en ;
      rdfs:label "Mass"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Name
:Name rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      rdfs:comment "The attribute that identifies the name of an object"@en ;
      rdfs:label "Name"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Orbital_Parameter
:Orbital_Parameter rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Orbiter ;
                   rdfs:range xsd:double ;
                   :physicsSymbol "a"^^xsd:string ;
                   rdfs:comment "The sum of the periapsis and apoapsis distances divided by two."@en ;
                   rdfs:label "Semimajor Axis"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Radius
:Radius rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Semimajor_Axis
:Semimajor_Axis rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :Orbital_Parameter ;
                rdfs:range xsd:double ;
                :physicsSymbol "a"^^xsd:string ;
                rdfs:comment "The sum of the periapsis and apoapsis distances divided by two."@en ;
                rdfs:label "Semimajor Axis"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#True_Anomaly
:True_Anomaly rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :Orbital_Parameter ;
              rdfs:range xsd:double ;
              :physicsSymbol "f"^^xsd:string ,
                             "θ"^^:greekLetter ,
                             "ν"^^xsd:string ;
              rdfs:comment "defines the position of the orbiting body along the ellipse at a specific time"@en ;
              rdfs:label "True Anomaly"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Areocentric
:Areocentric rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :Has_Orbit_Center ;
                                   owl:someValuesFrom [ owl:intersectionOf ( :Planet
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :Name ;
                                                                               owl:hasValue "Mars"
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ]
                                 ] ;
             rdfs:subClassOf :Orbiter ;
             dc:description "An Aerocentric Orbiter is an Astronomical Object that orbits around the planet Mars"@en ;
             rdfs:label "Aerocentric"@en ,
                        "Aerocentrico"@it ;
             rdfs:seeAlso "https://en.wikipedia.org/wiki/Areocentric_orbit"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Artificial_Satellite
:Artificial_Satellite rdf:type owl:Class ;
                      rdfs:subClassOf :Satellite ;
                      owl:disjointWith :Natural_Satellite ;
                      dc:description "A man-made apparatus designed to be placed in orbit around a celestial body, generally to relay information, data etc. to Earth."@en ;
                      rdfs:comment "A man-made apparatus designed to be placed in orbit around a celestial body, generally to relay information, data etc. to Earth."@it ;
                      rdfs:label "Artificial Satellite"@en ,
                                 "Satellite Artificiale"@it ;
                      rdfs:seeAlso <https://en.wikipedia.org/wiki/Satellite> ,
                                   "https://en.wikipedia.org/wiki/Satellite"@it .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Asteroid
:Asteroid rdf:type owl:Class ;
          rdfs:subClassOf :Astronomical_Object ;
          dc:description "Asteroids are actually minor planets which can neither be classified either as a planet or as a comet."@en ,
                         "Asteroids are small Celestial body with a composition similar to a Terrestrial planet, generally not sperical shape"@en ;
          rdfs:label "Asteroid"@en ,
                     "Asteroide"@it ;
          rdfs:seeAlso "https://en.wikipedia.org/wiki/Asteroid" .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Astronomical_Object
:Astronomical_Object rdf:type owl:Class ;
                     dc:description "Astronomical Body is a naturally occuring physical entity, association, or structure that exists in the observable universe."@en ;
                     rdfs:label "Astronomical Object"@en ,
                                "Oggetto Astronomico"@it ;
                     rdfs:seeAlso "https://en.wikipedia.org/wiki/Astronomical_object" .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Black_Hole
:Black_Hole rdf:type owl:Class ;
            rdfs:subClassOf :Star ;
            dc:description "A black hole is a region of spacetime exhibiting gravitational acceleration so strong that nothing, no particles or even electromagnetic radiation such as light, can escape from it."@en ;
            rdfs:label "Black Hole"@en ,
                       "Buco nero"@it ;
            rdfs:seeAlso "https://en.wikipedia.org/wiki/Black_hole"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Comet
:Comet rdf:type owl:Class ;
       rdfs:subClassOf :Astronomical_Object ;
       dc:description "A comet is an icy, small Solar System body that, when passing close to the Sun, warms and begins to release gases, a process called outgassing."@en ,
                      "Comets sometimes can have one or two tails"@en ;
       rdfs:label "Comet"@en ,
                  "Cometa"@it ;
       rdfs:seeAlso "https://en.wikipedia.org/wiki/Comet"@it .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Dwarf_Planet
:Dwarf_Planet rdf:type owl:Class ;
              rdfs:subClassOf :Astronomical_Object ;
              dc:description "A dwarf planet is a planetary-mass object that does not dominate its region of space and is not a satellite."@en ;
              rdfs:label "Dwarf Planet"@en ,
                         "Pianeta Nano"@it ;
              rdfs:seeAlso "https://en.wikipedia.org/wiki/Dwarf_planet"@it .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Geocentric
:Geocentric rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :Has_Orbit_Center ;
                                  owl:someValuesFrom [ owl:intersectionOf ( :Planet
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :Name ;
                                                                              owl:hasValue "Earth"
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                ] ;
            rdfs:subClassOf :Orbiter ;
            dc:description "A Geocentric Orbiter is an Astronomical Object that orbits around the planet Earth"@en ;
            rdfs:label "Geocentric"@en ,
                       "Geocentrico"@it ;
            rdfs:seeAlso <https://en.wikipedia.org/wiki/Geocentric_orbit> .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Giant_Planet
:Giant_Planet rdf:type owl:Class ;
              rdfs:subClassOf :Planet ;
              owl:disjointWith :Terrestrial_Planet ;
              dc:description "A giant planet is any planet much larger than Earth."@en ;
              rdfs:label "Giant Planet"@en ,
                         "Pianeta Gigante"@it ;
              rdfs:seeAlso "https://en.wikipedia.org/wiki/Giant_planet"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Habitable_Planet
:Habitable_Planet rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Solar_System_Planet
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :Semimajor_Axis ;
                                                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                    owl:onDatatype xsd:double ;
                                                                                    owl:withRestrictions ( [ xsd:minInclusive "0.949999998897"^^xsd:double
                                                                                                           ]
                                                                                                           [ xsd:maxInclusive "1.37000613071"^^xsd:double
                                                                                                           ]
                                                                                                         )
                                                                                  ]
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Solar_System_Planet ;
                  dc:description "Habitable Planet is a planet that is able to develop and maintain environments hospitable to life."@en ;
                  rdfs:label "Habitable Planet"@en ,
                             "Pianeta Abitabile"@it ;
                  rdfs:seeAlso "https://en.wikipedia.org/wiki/Planetary_habitability"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Heliocentric
:Heliocentric rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :Has_Orbit_Center ;
                                    owl:someValuesFrom [ owl:intersectionOf ( :Star
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty :Name ;
                                                                                owl:hasValue "Sun"
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ;
              rdfs:subClassOf :Orbiter ;
              dc:description "An Aerocentric Orbiter is an Astronomical Object that orbits around the Star Sun"@en ;
              rdfs:isDefinedBy "https://en.wiktionary.org/wiki/heliocentric"@en ;
              rdfs:label "Eliocentrico"@it ,
                         "Heliocentric"@en .


###  http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#Inhabitable_Planet
:Inhabitable_Planet rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:unionOf ( [ owl:intersectionOf ( :Solar_System_Planet
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty :Semimajor_Axis ;
                                                                                 owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                      owl:onDatatype xsd:double ;
                                                                                                      owl:withRestrictions ( [ xsd:maxInclusive "0.949999998897"^^xsd:double
                                                                                                                             ]
                                                                                                                           )
                                                                                                    ]
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :Semimajor_Axis ;
                                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                               owl:onDatatype xsd:double ;
                                                                               owl:withRestrictions ( [ xsd:minInclusive "1.37000613071"^^xsd:double
                                                                                                      ]
                                                                                                    )
                                                                             ]
                                                        ]
                                                      )
                                        ] ;
                    rdfs:subClassOf :Solar_System_Planet ;
                    dc:description "Habitable Planet is a planet that is not able to develop and maintain environments hospitable to life."@en ;
                    rdfs:label "Inhabitable Planet"@en ,
                               "Pianeta Inabitabile"@it ;
