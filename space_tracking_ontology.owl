                                                                                dc:date "2020-01-14T15:36:00+01:00"^^xsd:dateTime ;
                                                                                dc:description "Ontology made to organize and track astronomical objects all around the solar system and beyond"^^xsd:string ;
                                                                                dc:title "Space Tracking Ontology"^^xsd:string ;
                                                                                vann:preferredNameSpacePrefix "STO"^^xsd:string ;
                                                                                vann:preferredNameSpaceURI "http://www.semanticweb.org/daniele/ontologies/2020/4/space-tracking-ontology#"^^xsd:string ;
                                                                                rdfs:label "Space Tracking"^^xsd:string ,
                                                                                           "Space Tracking Ontology"^^xsd:string .

